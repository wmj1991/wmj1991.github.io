.list{
  width:100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.searchinp{
  width:96%;
  height: 90rpx;
  text-align: center;
  background-color: #eee;
  margin:0 2%
}
 .nav{
  width:100%;
  height:100rpx;
  display: flex;
}
.nav-item{
  flex: 1;
  text-align: center;
  line-height: 99rpx;
}
.selected-nav-item{
  background:#1296db;
}
.swiper{
  flex: 1;
  overflow-y: scroll;
}
swiper-item{
  overflow-y: scroll;
}



.box{
  width:100%;
  display: flex;
  flex-wrap: wrap;
}
.card{
  width:48%;
  font-size: 30rpx;
  margin:0 1%;
}
.img{
  width:100%;
}
.red{
  color: red;
}
.text{
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.ccc{
  color:#999;
}
.card-nav{
  width: 100%;
}

